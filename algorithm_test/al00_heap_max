import heapq

# 오름차순 Heap Sort
def heapsort(iterable):
    h = []
    result = []
    # 모든 원소 차례로 Heap에 삽입
    for value in iterable:
        heapq.heappush(h, value)
    # 힙에 삽입된 모든 원소 차례대로 꺼내어 담기
    for i in range(len(h)):
        result.append(heapq.heappop(h))
    return result

result = heapsort([5, 3, 1, 9, 7, 4, 2, 6, 8, 0])
print(result) 
